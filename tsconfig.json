{
  "compileOnSave": false,
  "compilerOptions": {
    "jsx": "react",
    "jsxFactory": "Snabbdom.createElement",
    "lib": [
      "dom",
      "es2015",
      "es2015.promise"
    ],
    "moduleResolution": "node",
    "module": "commonjs",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es5",

    "typeRoots": [
      "./types"
    ]
  }
}
